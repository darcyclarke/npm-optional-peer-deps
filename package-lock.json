{
  "name": "npm-optional-peer-deps",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "version": "1.0.0",
      "peerDependencies": {
        "a": "file:./a",
        "b": "file:./b",
        "react": "*"
      },
      "peerDependenciesMeta": {
        "react": {
          "optional": true
        }
      }
    },
    "a": {
      "version": "1.0.0",
      "peer": true,
      "peerDependencies": {
        "express": ">=1"
      },
      "peerDependenciesMeta": {
        "express": {
          "optional": true
        }
      }
    },
    "b": {
      "version": "1.0.0",
      "peer": true,
      "peerDependencies": {
        "express": "2.0.0"
      },
      "peerDependenciesMeta": {
        "express": {
          "optional": true
        }
      }
    },
    "node_modules/a": {
      "resolved": "a",
      "link": true
    },
    "node_modules/b": {
      "resolved": "b",
      "link": true
    }
  },
  "dependencies": {
    "a": {
      "version": "file:a",
      "requires": {}
    },
    "b": {
      "version": "file:b",
      "requires": {}
    }
  }
}
